                    android:text="邀请好友"
                    android:textColor="@color/white"
                    android:visibility="gone"
                    android:background="@color/dlg_red_title_color"
                    android:textSize="@dimen/h5"
                    android:layout_width="58dp"
                    android:layout_height="26dp"/>-->


        </FrameLayout>

        <LinearLayout
                android:orientation="vertical"
                android:paddingLeft="10dp"
                android:paddingRight="10dp"
                android:layout_width="match_parent"
                android:layout_height="wrap_content">


            <LinearLayout
                    android:orientation="vertical"
                    android:background="#30000000"
                    android:layout_width="80dp"
                    android:layout_height="wrap_content">

                <TextView
                        android:layout_marginLeft="5dp"
                        android:id="@+id/tv_index_third_item_id"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="ID:"
                        android:textColor="@color/tv_write"
                        android:textSize="@dimen/h7"/>
                <TextView
                        android:singleLine="true"
                        android:layout_marginLeft="5dp"
                        android:id="@+id/tv_index_third_item_price"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:gravity="left"
                        android:text="¥："
                        android:textColor="@color/tv_write"
                        android:textSize="@dimen/h7"/>
            </LinearLayout>

            <ProgressBar
                    android:id="@+id/pb_index_third_item_bar"
                    style="@style/pb_style"
                    android:layout_marginTop="10dp"
                    android:layout_marginBottom="10dp"
                    android:max="100"
                    android:progress="0"/>

            <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="15dp"
                    android:layout_marginBottom="15dp"
                    android:orientation="horizontal">
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tv_index_third_item_join"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="0"
                        android:textColor="@color/dlg_red_title_color"
                        android:textSize="@dimen/h6"/>

                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="已参与"
                        android:textColor="@color/black_light_color"
                        android:textSize="@dimen/h6"/>
                </LinearLayout>
                <TextView
                    android:background="@color/cutline"
                    android:layout_gravity="center"
                    android:layout_width="1dp"
                    android:layout_height="17dp" />
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tv_index_third_item_all"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="@color/black_light_color"
                        android:textSize="@dimen/h6"/>

                    <TextView
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:layout_gravity="center"
                        android:gravity="center"
                        android:text="总需人次"
                        android:textColor="@color/black_light_color"
                        android:textSize="@dimen/h6"/>
                </LinearLayout>
                <TextView
                    android:background="@color/cutline"
                    android:layout_gravity="center"
                    android:layout_width="1dp"
                    android:layout_height="17dp" />
                <LinearLayout
                    android:layout_width="0dp"
                    android:layout_weight="1"
                    android:layout_height="wrap_content"
                    android:orientation="vertical">
                    <TextView
                        android:id="@+id/tv_index_third_item_release"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:text="0"
                        android:textColor="@color/blue_item_show"
                        android:textSize="@dimen/h6"/>
                    <TextView
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_gravity="right"
                        android:gravity="right"
                        android:text="剩余"
                        android:textColor="@color/black_light_color"
                        android:textSize="@dimen/h6"/>
                </LinearLayout>

            </LinearLayout>

            <!--<FrameLayout-->
                    <!--android:layout_width="match_parent"-->
