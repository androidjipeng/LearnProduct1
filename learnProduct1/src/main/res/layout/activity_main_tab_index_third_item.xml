<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
             android:layout_width="match_parent"
             android:layout_height="wrap_content"
             android:background="@drawable/white_no_corners"
             android:orientation="vertical">

    <LinearLayout
            android:id="@+id/area_index_third_item"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="@drawable/bg_common_selector"
            android:orientation="vertical" >

        <FrameLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content">

            <ImageView
                    android:id="@+id/iv_index_third_item_pic"
                    android:layout_width="match_parent"
                    android:layout_height="120dp"
                    android:layout_marginLeft="25dp"
                    android:layout_marginRight="25dp"
                    android:src="@drawable/ic_no_loading_default"/>
              <!--iv_index_third_item_delete点击效果bg_common_selector-->
            <ImageView
                    android:id="@+id/iv_index_third_item_delete"
                    android:layout_gravity="top|right"
                    android:background="@drawable/delete"
                    android:layout_margin="5dp"
                    android:visibility="gone"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"/>
            <!--<TextView
                    android:id="@+id/iv_index_third_item_share"
                    android:layout_gravity="right|bottom"
